<%- include("partials/header.ejs") %>
<h1>Home</h1>
<p><%= homeContent %> </p>

<% posts.forEach((post) => { %>
    <div class="postStyle">
    <h2><%= post.title %></h2>
    <p>
        <%=post.content.substring(0, 100) + "..."%>
        <a href="/posts/<%= post.title %>">Read More</a>
    </p>
    </div>
<% }); %>

<%- include("partials/footer.ejs") %>